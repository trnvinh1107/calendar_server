CREATE TABLE users (
    userName VARCHAR(50) PRIMARY KEY,
    firstName NVARCHAR(50),
    lastName NVARCHAR(50),
    email VARCHAR(255),
    password VARCHAR(255),
    isAdmin BOOLEAN
);

CREATE TABLE rooms (
	id VARCHAR(50) PRIMARY KEY,
	name NVARCHAR(50),
	capacity INT,
	description NVARCHAR(255),
	isUsing BOOLEAN,
	isActivity BOOLEAN
);